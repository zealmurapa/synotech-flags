{
  "name": "@synotech/flags",
  "version": "3.0.1",
  "description": "circular country flags, compatible with React Native and React.js",
  "main": "dist/react/index.js",
  "module": "dist/react/index.js",
  "types": "dist/react/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "exports": {
    ".": {
      "types": "./dist/react/index.d.ts",
      "react-native": "./dist/react-native/index.js",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    },
    "./react-native": {
      "types": "./dist/react-native/index.d.ts",
      "import": "./dist/react-native/index.js",
      "require": "./dist/react-native/index.cjs"
    }
  },
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "yarn build",
    "build": "tsup",
    "build:rn": "tsup --config tsup.config.ts --entry.react-native src/index.tsx --outDir lib/react-native",
    "build:react": "tsup --config tsup.config.ts --entry.react src/index.react.tsx --outDir lib/react",
    "release": "release-it",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install"
  },
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!dist/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "react",
    "reactjs",
    "flags",
    "svg",
    "ios",
    "android",
    "web"
  ],
  "repository": "https://github.com/synotech-ai/synotech-flags",
  "author": "Zeal Murapa <contact@synotech.cloud> (https://synotech.cloud)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/synotech-ai/synotech-flags/issues"
  },
  "homepage": "https://github.com/synotech-ai/synotech-flags#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",   
    "@react-native-community/eslint-config": "^3.0.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@svgr/cli": "^8.1.0",
    "@types/jest": "^28.1.2",
    "@types/node": "^24.0.14",
    "@types/react": "18",
    "@types/react-dom": "^19.1.6",
    "@types/react-native": "0.70.0",
    "commitlint": "^17.0.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18",
    "react-dom": "^19.1.0",
    "react-native": "0.72.4",
    "react-native-builder-bob": "^0.20.0",
    "react-native-svg": "^13.12.0",
    "release-it": "^15.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-svg": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/dist/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "off",
        {
          "bracketSpacing": true,
          "tabWidth": 4,
          "useTabs": true,
          "bracketSameLine": true,
          "singleQuote": true,
          "semi": false,
          "trailingComma": "es5",
          "endOfLine": "auto",
          "printWidth": 120
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "prettier": {
    "bracketSpacing": true,
    "tabWidth": 4,
    "useTabs": true,
    "bracketSameLine": true,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "endOfLine": "auto",
    "printWidth": 120
  }
}
